{
  "expo": {
    "name": "CoTruck Operator",
    "slug": "co-truck-operator",
    "scheme": "untitled-app",
    "description": "This app powered by AGGA.io",
    "privacy": "unlisted",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/app/icon.png",
    "backgroundColor": "#FBFCFD",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5A45FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "ALL"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.draftbit.untitledapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to review rich media previously added.",
        "NSUserTrackingUsageDescription": "This app uses the location to create a list of local services.",
        "NSPhotoLibraryAddUsageDescription": "This app uses the photo library to add rich media shared from others.",
        "NSLocationWhenInUseUsageDescription": "This app uses the location to create a list of local services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses the location to create a list of local services."
      },
      "usesIcloudStorage": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.draftbit.untitledapp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "VIBRATE"
      ],
      "adaptiveIcon": {}
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "androidNavigationBar": {},
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}